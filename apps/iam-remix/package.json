{
  "name": "@kurocado-studio/iam-remix-demo",
  "private": true,
  "version": "0.0.0-semantically-released",
  "type": "module",
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "build": "rm -rf dist && remix vite:build",
    "clean": "rm -rf .turbo node_modules",
    "dev": "remix vite:dev",
    "lint": "eslint --max-warnings=0 . --no-warn-ignored",
    "lint:fix": "eslint --max-warnings=0 . --fix --no-warn-ignored",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --check . --write",
    "start": "remix-serve ./build/server/index.js",
    "setup": "pnpm install",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@kurocado-studio/auth-zero": "workspace:*",
    "@remix-run/node": "^2.16.0",
    "@remix-run/react": "^2.16.0",
    "autoprefixer": "^10.4.21",
    "isbot": "^5.1.23",
    "lodash-es": "^4.17.21",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@kurocado-studio/qa": "2.0.0-alpha.2",
    "@kurocado-studio/styleguide": "^2.3.3",
    "@remix-run/dev": "^2.16.0",
    "@remix-run/serve": "^2.16.0",
    "@vitest/coverage-v8": "^3.0.8",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "semantic-release-monorepo": "^8.0.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vitest": "^3.0.8"
  }
}
